{
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/Settings.php"
        ]
    },
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project template for Drupal 8 projects with composer",
    "extra": {
        "drupal-scaffold": {
            "excludes": [
                ".htaccess",
                "update.php"
            ]
        },
        "installer-paths": {
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    },
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "package": {
                "name": "components/highlightjs",
                "source": {
                    "reference": "9.7.0",
                    "type": "git",
                    "url": "https://github.com/components/highlightjs"
                },
                "type": "drupal-library",
                "version": "9.7.0"
            },
            "type": "package"
        },
        {
            "package": {
                "name": "d3/d3",
                "source": {
                    "reference": "v3.5.17",
                    "type": "git",
                    "url": "https://github.com/d3/d3"
                },
                "type": "drupal-library",
                "version": "v3.5.17"
            },
            "type": "package"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.6",
        "drupal/adminimal_admin_toolbar": "~1.4",
        "drupal/adminimal_theme": "~1.5",
        "drupal/config_filter": "~1.0",
        "drupal/config_split": "~1.0",
        "drupal/console": "^1.9.4",
        "drupal/core": "~8.8",
        "drupal/redis": "^1.0",
        "drush/drush": "^9.0.0",
        "niji/starter_kit": "~1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.2.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "components/highlightjs": "9.7.*",
        "d3/d3": "3.5.*",
        "drupal/devel": "~2.1",
        "drupal/vardumper": "^1.4",
        "webflo/drupal-core-require-dev": "~8.8"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
